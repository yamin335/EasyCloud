<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/support_graph"
    app:startDestination="@id/supportScreen"
    tools:ignore="UnusedNavigation">

    <fragment
            android:id="@+id/supportScreen"
            android:name="ltd.royalgreen.pacecloud.supportmodule.SupportFragment"
            android:label="@string/support"
            tools:layout="@layout/support_fragment">
        <action android:id="@+id/action_supportScreen_to_about_graph" app:destination="@id/about_graph"/>
    </fragment>
    <include app:graph="@navigation/about_graph"/>
</navigation>
