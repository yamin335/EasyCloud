<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/support"
    app:startDestination="@id/register"
    tools:ignore="UnusedNavigation">

    <fragment
            android:id="@+id/register"
            android:name="ltd.royalgreen.pacecloud.formscreen.Register"
            android:label="@string/support"
            tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_register_to_registered"
            app:destination="@id/registered"/>
    </fragment>
    <fragment
        android:id="@+id/registered"
        android:name="ltd.royalgreen.pacecloud.formscreen.Registered"
        android:label="Registered"
        tools:layout="@layout/fragment_registered"/>
</navigation>
